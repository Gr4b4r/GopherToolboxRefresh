@model IEnumerable<GopherToolboxRefresh.Models.Order>

@{
	
}

<h1>Zarządzanie użytkownikami</h1>
<div class="card adminSets">
	<table class="table">
		<thead>
			<tr>
				<th>Id użytkownika</th>
				<th>Imię</th>
				<th>Ksywka</th>
				<th>Nazwisko</th>
				<th>Data ur.</th>
				<th>Email</th>
				<th>Telefon</th>
				<th>Akcje</th>
			</tr>
		</thead>
		<tbody>
		
			{
				<tr>
					<td>item.Id</td>
					<td>item.Name</td>
					<td>item.Nickname</td>
					<td>item.Surname</td>
					<td>item.BirthDate</td>
					<td>item.Email</td>
					<td>item.Phone</td>
					<td width="250px;">
						<a asp-action="Edit" asp-route-id="item.Id" class="btn btn-warning">Edytuj</a>
						<a asp-action="Details" asp-route-id="item.Id" class="btn btn-primary">Szczegóły</a>
						<a asp-action="Delete" asp-route-id="item.Id" class="btn btn-danger">Usuń</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>
